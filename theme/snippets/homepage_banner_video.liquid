{% if settings.video_banner_url != blank %}
<div class="banner banner-video embed-responsive embed-responsive-16by9">
  <div class="banner-video-player" id="video-player" data-property="{ videoURL:'{{ settings.video_banner_url }}', containment:'.banner-video', autoPlay: true, mute: true, startAt: 0, opacity: 1, showControls: false, optimizeDisplay:false, stopMovieOnBlur: false }"></div>
</div>
{% endif %}